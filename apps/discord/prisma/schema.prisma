datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator kysely {
    provider = "prisma-kysely"
    fileName = "types.ts"
}

model User {
    id Int @id @default(autoincrement())
}

model Guild {
    id      String  @id
    enabled Boolean @default(true)
    format  Format  @default(DEFAULT)
}

enum Format {
    DEFAULT
    MINIMAL
}
